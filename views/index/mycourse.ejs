<%- partial ('../common/index-header.ejs') %>
<div class="container">
	<div class="row">
		<h2>Khóa học đã mua</h2>
		<% for(var i=0; i < orderCourse.length; i++) { %>
		<div class="col-md-4 col-sm-6 col-xs-12">
			 <a href="/course-detail/<%=orderCourse[i].course.id%>" class="thumbnail relative text-decoration-none">
				<img src="<%= orderCourse[i].course.picture%>" class="img-responsive" alt="" />
				<h5 class="course-title"><%= orderCourse[i].course.name%></h5>
			 </a>
		</div>
		<% } %>
	</div>
</div>
<!-- /.container -->
